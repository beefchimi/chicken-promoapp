%section#available.available

	%article{"data-container" => "width_1200"}

		%h4 Available for iPhone 5 and up and Android 4.0.3 and up. Also available for iPad and Android tablets.

		.wrap_svg
			%svg.ui_devices{:viewBox => "0 0 422 154"}
				%use{"xlink:href" => "#ui_devices"}

		.links_download.clearfix

			%a.button.white.download_ios{:href => "#", :title => "Download for iOS"}
				%span Download Now for iOS

			%a.button.white.download_android{:href => "#", :title => "Download for Android"}
				%span Download Now for Android